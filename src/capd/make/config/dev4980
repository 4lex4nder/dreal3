# settings for Dev-C++ 4.9.8.0 in MS-Windows

GDIR    = C:/DEV-CPP
SYSLIB  = -L${GDIR}/lib/
SYSLIBG = -L${GDIR}/lib/
SYSINCL = -I${GDIR}/include/c++ -I${GDIR}/include \
	-I${GDIR}/include/c++/backward -I${GDIR}/include/c++/mingw32
COMPILE = g++ -mwindows -O2 
LINK    = g++ -s
LINKGUI = g++ -mwindows -mconsole -s
MAKELIB = ar cru
IDXLIB  = ranlib
REMOVE  = perl ${HOME}make/slashes.pl del
MKDIR   = perl ${HOME}make/slashes.pl mkdir
EXE     = .exe
LIBEXT  = .a
OBJEXT  = .o
